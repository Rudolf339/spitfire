  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>Lever-Rad-Flap</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/cowl-flaps-norm</property>
        <step type="double">0.25</step>
        <min type="double">0</min>
        <max type="double">1.25</max>
        <wrap type="bool">true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Lever-Rad-Flap</object-name>
    <property>controls/engines/engine/cowl-flaps-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.25</ind>
        <dep>5</dep>
      </entry>
      <entry>
        <ind>0.5</ind>
        <dep>10</dep>
      </entry>
      <entry>
        <ind>0.75</ind>
        <dep>15</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>20</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>3.74</x1-m>
      <y1-m>0.19</y1-m>
      <z1-m>-0.80</z1-m>
      <x2-m>3.74</x2-m>
      <y2-m>-0.19</y2-m>
      <z2-m>-0.80</z2-m>
    </axis>
  </animation>

  <!-- ***************** Seat Height*********************** -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>Seat-Adjust</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/seat/vertical-adjust</property>
        <step type="double">0.01</step>
        <min type="double">-0.05</min>
        <max type="double">0.05</max>
        <wrap type="bool">true</wrap>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>sim/current-view/view-number</property>
            <value>0</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>sim/current-view/y-offset-m</property>
        <step type="double">0.01</step>
        <min type="double">0.477</min>
        <max type="double">0.5765</max>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Seat-Adjust</object-name>
    <property>/controls/seat/vertical-adjust</property>
    <interpolation>
      <entry>
        <ind>-0.05</ind>
        <dep>25</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>35</dep>
      </entry>
      <entry>
        <ind>0.05</ind>
        <dep>45</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>4.10254</x-m>
      <y-m>0.253859</y-m>
      <z-m>-0.403489</z-m>
    </center>
  </animation>

  <!-- ***************** Map Box*********************** -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>Map-Box</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>dialog-show</command>
        <dialog-name>map</dialog-name>
      </binding>
    </action>
  </animation>

