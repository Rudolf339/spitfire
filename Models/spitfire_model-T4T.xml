      <entry>
        <ind>0.005</ind>
        <dep>45</dep>
      </entry>
      <entry>
        <ind>0.11</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>3.59456</x1-m>
      <y1-m>-0.0140342</y1-m>
      <z1-m>0.635517</z1-m>
      <x2-m>3.59456</x2-m>
      <y2-m>0.0140342</y2-m>
      <z2-m>0.635517</z2-m>
